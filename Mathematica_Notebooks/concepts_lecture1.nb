Cell[CellGroupData[{

Cell["\<\
Time evolution as above obtained using eigenstates x_m(n)=sum_i  <m| i><i|1> \
exp(i t E_i)\
\>", "Subsection",ExpressionUUID->"bda71d69-042c-4bf6-8930-2bd4ddb5d2bb"],

Cell["normalize eigenvectors", "Text",ExpressionUUID->"b7914d69-6f03-43d2-a17d-79d02932b74a"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"evec", " ", "=", " ", 
   RowBox[{"Module", "[", "\n", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"x", " ", "=", " ", 
       RowBox[{"eig", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], "}"}], ",", " ", "\n", 
     RowBox[{
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
        RowBox[{"i", " ", "<", " ", "7"}], ",", " ", 
        RowBox[{"i", "++"}], ",", "\n", 
        RowBox[{
         RowBox[{"y", " ", "=", " ", 
          RowBox[{"Sqrt", "[", 
           RowBox[{
            RowBox[{"x", "[", 
             RowBox[{"[", "i", "]"}], "]"}], ".", 
            RowBox[{"x", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}], ";", "\n", 
         RowBox[{
          RowBox[{"x", "[", 
           RowBox[{"[", "i", "]"}], "]"}], " ", "=", " ", 
          RowBox[{
           RowBox[{"x", "[", 
            RowBox[{"[", "i", "]"}], "]"}], " ", "/", " ", "y"}]}]}]}], "]"}],
       ";", "\n", "x"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Quant2", "[", 
    RowBox[{"n_", ",", " ", "m_"}], "]"}], " ", ":=", " ", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"evec", "[", 
        RowBox[{"[", "i", "]"}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}], " ", "*", " ", 
      RowBox[{
       RowBox[{"evec", "[", 
        RowBox[{"[", "i", "]"}], "]"}], "[", 
       RowBox[{"[", "m", "]"}], "]"}], " ", "*", " ", 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "\[ImaginaryI]"}], " ", "*", " ", "n", " ", "*", " ", 
        "x", " ", "*", " ", 
        RowBox[{"ee", "[", 
         RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"i", ",", " ", "1", ",", " ", "6"}], "}"}]}], "]"}]}], 
  ";"}], "\n", 
 RowBox[{"ListPlot", "[", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Abs", "[", 
        RowBox[{"Quant2", "[", 
         RowBox[{"n", ",", " ", "1"}], "]"}], "]"}], "^", "2"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"n", ",", " ", "0", ",", " ", "100"}], "}"}]}], "]"}], ",", 
    RowBox[{"Quant", "[", 
     RowBox[{"100", ",", "1"}], "]"}]}], "}"}], "]"}]}], "Input",ExpressionUUI\
D->"cebf5759-ff80-4266-9ece-e55dd860ca0d"],

