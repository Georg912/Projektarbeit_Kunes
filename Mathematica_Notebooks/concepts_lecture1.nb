     RowBox[{
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", " ", "=", " ", "0"}], ",", " ", 
        RowBox[{"i", " ", "<", " ", "n"}], ",", " ", 
        RowBox[{"i", "++"}], ",", " ", 
        RowBox[{
         RowBox[{"x", " ", "=", " ", 
          RowBox[{"U", ".", "x"}]}], ";", "\n", 
         RowBox[{"y", " ", "=", " ", 
