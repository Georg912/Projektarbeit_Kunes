Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Quant", "[", 
    RowBox[{"n_", ",", " ", "m_"}], "]"}], " ", ":=", " ", 
   RowBox[{"Module", "[", "\n", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
